{
  "name": "EditorConfig",
  "description": "Detects EditorConfig files and adheres to defined styles.",
  "version": "0.0.3",
  "publisher": "EditorConfig",
  "icon": "logo.png",
  "galleryBanner.color": "f0f0f0",
  "repository": {
     "type": "git",
     "url": "git+https://github.com/editorconfig/editorconfig-vscode"
  },
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.editorconfig"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "editorconfig": "^0.13.0",
    "lodash.compact": "^3.0.0"
  }
}
